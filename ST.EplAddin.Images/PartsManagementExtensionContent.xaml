             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EplAddin.Article_AddImageContextDialog"
             DataContext="{Binding RelativeSource={RelativeSource Self}, Path=ViewModel}"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="638">
    <Grid>
        <Image x:Name="img"  HorizontalAlignment="Left" Height="355" Margin="10,85,0,0" VerticalAlignment="Top" Width="618"/>
        <Button x:Name="clip" Content="Clipboard" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="185" Click="Button_Click" Height="47"/>
        <Button x:Name="save" Content="Save" HorizontalAlignment="Left" Margin="268,10,0,0" VerticalAlignment="Top" Width="168" Height="47"/>

    </Grid>
</UserControl>
